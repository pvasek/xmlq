{
  "name": "@pvasek/xmlq",
  "version": "0.0.1",
  "description": "A pipe-friendly CLI for exploring and querying XML files",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "xmlq": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm test"
  },
  "files": [
    "dist",
    "skills"
  ],
  "keywords": [
