    "cli",
    "xpath",
    "query"
  ],
  "author": "pvasek",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pvasek/xmlq.git"
  },
  "homepage": "https://github.com/pvasek/xmlq#readme",
  "bugs": {
    "url": "https://github.com/pvasek/xmlq/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@xmldom/xmldom": "^0.8.11",
    "commander": "^14.0.3",
    "fast-xml-parser": "^5.3.5",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
